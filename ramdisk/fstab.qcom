# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

#TODO: Add 'check' as fs_mgr_flags with data partition.
# Currently we dont have e2fsck compiled. So fs check would failed.

#Zhilong.Zhang@OnlineRd.Driver, 2013/11/09, Add for support OTG device 
#<src>                                                <mnt_point>  <type>  <mnt_flags and options>                     <fs_mgr_flags>
/dev/block/bootdevice/by-name/system         /system      ext4    ro,barrier=1,discard                                wait
#ifdef VENDOR_EDIT
#Junren_Jie@EXP.framework, 2015/06/01, add 16384 space reserved for data partitiong, modify for support entire disk encryption 
/dev/block/bootdevice/by-name/userdata       /data        ext4    nosuid,nodev,barrier=1,noauto_da_alloc,discard      wait,check,encryptable=footer,length=-16384
#endif /* VENDOR_EDIT */
/devices/soc.0/7864900.sdhci/mmc_host        /storage/sdcard1   vfat    nosuid,nodev         wait,voldmanaged=sdcard1:auto,noemulatedsd
/dev/block/zram0             none         swap    defaults zramsize=536870912
/dev/block/bootdevice/by-name/config         /frp         emmc    defaults                                            defaults
#ifdef VENDOR_EDIT
#LinJie.Xu@Prd.Android.USB, 2015/05/11, Add for support OTG
/devices/platform/msm_hsusb                  /storage/UDiskA    vfat    nosuid,nodev         wait,voldmanaged=UDiskA:auto
